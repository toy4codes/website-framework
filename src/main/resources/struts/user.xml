<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="user" namespace="/user" extends="json-default">

		<action name="save" class="userAction" method="save">
			<result>/jsp/user/loadAll.jsp</result>
		</action>

		<action name="delete" class="userAction" method="delete">
			<result>/jsp/user/loadAll.jsp</result>
		</action>

		<action name="update" class="userAction" method="update">
			<result>/jsp/user/loadAll.jsp</result>
		</action>

		<action name="load" class="userAction" method="load">
			<result>/jsp/user/load.jsp</result>
		</action>

		<action name="loadAll" class="userAction" method="loadAll">
			<result type="json"></result>
		</action>

	</package>

</struts>